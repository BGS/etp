#include "includes/package/pkgutils.hpp"

pkgutil::EtpPkgUtils::EtpPkgUtils()
{

}

pkgutil::EtpPkgUtils::~EtpPkgUtils()
{

}

bool pkgutil::EtpPkgUtils::bz2_extract_package(const char* package_path)
{
    ifstream package(package_path, , ios_base::in | ios_base::binary);
    ofstream test;

    filtering_streambuf<input> in;
    in.push(bzip2_decompressor());
    in.push(package);
    boost::iostreams::copy(in, test);

    return true;


}
